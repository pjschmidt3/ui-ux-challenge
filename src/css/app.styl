@font-face {
  font-family 'Scandia'
  src url('fonts/Scandia-Bold.eot')
  src url('fonts/Scandia-Bold.eot?#iefix') format('embedded-opentype'),
  url('fonts/Scandia-Bold.woff2') format('woff2'),
  url('fonts/Scandia-Bold.woff') format('woff'),
  url('fonts/Scandia-Bold.ttf') format('truetype'),
  url('fonts/Scandia-Bold.svg#Scandia-Bold') format('svg')
  font-weight bold
  font-style normal
}

@font-face {
  font-family 'Scandia'
  src url('fonts/Scandia-Regular.eot')
  src url('fonts/Scandia-Regular.eot?#iefix') format('embedded-opentype'),
  url('fonts/Scandia-Regular.woff2') format('woff2'),
  url('fonts/Scandia-Regular.woff') format('woff'),
  url('fonts/Scandia-Regular.ttf') format('truetype'),
  url('fonts/Scandia-Regular.svg#Scandia-Regular') format('svg')
  font-weight normal
  font-style normal
}

body {
  font-family 'Scandia'
}
